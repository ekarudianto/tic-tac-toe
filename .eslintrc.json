{
    "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
    ],
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "ignore": [ "config/", "model/", "i8n/" ]
        }
      ],
      "import/extensions": [
        ["error", "never", { "js": "always" }]
      ]
    }
}